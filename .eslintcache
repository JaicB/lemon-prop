                  </div>\n                )\n            )\n          }\n        </div>\n      )\n    })\n  }\n\n  return (\n    <div className='maint-req'>\n      {mapIt(currentTenants)}\n    </div>\n  )\n}\n\nfunction mapStateToProps(state) {\n  return {\n    email: state.user.email,\n    user_id: state.user.user_id,\n    admin: state.user.admin,\n    approved: state.user.approved\n  }\n}\n\nexport default connect(mapStateToProps)(TenantList)","/Users/jakebryant/DevMtn/group-project/lemon-prop/src/Components/Popup.js",[],["141","142"],{"ruleId":"143","replacedBy":"144"},{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","severity":1,"message":"148","line":14,"column":6,"nodeType":"149","endLine":14,"endColumn":8,"suggestions":"150"},{"ruleId":"151","severity":1,"message":"152","line":13,"column":9,"nodeType":"153","endLine":13,"endColumn":43},{"ruleId":"147","severity":1,"message":"154","line":27,"column":6,"nodeType":"149","endLine":27,"endColumn":15,"suggestions":"155"},{"ruleId":"156","severity":1,"message":"157","line":1,"column":27,"nodeType":"158","messageId":"159","endLine":1,"endColumn":36},{"ruleId":"156","severity":1,"message":"160","line":8,"column":10,"nodeType":"158","messageId":"159","endLine":8,"endColumn":15},{"ruleId":"156","severity":1,"message":"161","line":8,"column":12,"nodeType":"158","messageId":"159","endLine":8,"endColumn":26},{"ruleId":"156","severity":1,"message":"162","line":12,"column":13,"nodeType":"158","messageId":"159","endLine":12,"endColumn":20},{"ruleId":"156","severity":1,"message":"160","line":12,"column":22,"nodeType":"158","messageId":"159","endLine":12,"endColumn":27},{"ruleId":"156","severity":1,"message":"163","line":9,"column":12,"nodeType":"158","messageId":"159","endLine":9,"endColumn":24},{"ruleId":"156","severity":1,"message":"164","line":13,"column":35,"nodeType":"158","messageId":"159","endLine":13,"endColumn":41},{"ruleId":"147","severity":1,"message":"154","line":30,"column":6,"nodeType":"149","endLine":30,"endC